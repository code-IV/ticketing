{
  "name": "bora-ticketing-backend",
  "version": "1.0.0",
  "description": "Online Ticketing System for Bora Amusement Park - Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:status": "knex migrate:status",
    "db:reset": "node -e \"if(process.env.NODE_ENV==='production'){throw new Error('db:reset is not allowed in production')}\" && knex migrate:rollback --all && knex migrate:latest",
    "db:seed": "node src/database/seed.js"
  },
  "keywords": [
    "ticketing",
    "amusement-park",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.18.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
